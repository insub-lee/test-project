{
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "components/*": ["components/*"],
      "utils/*": ["utils/*"],
      "containers/*": ["containers/*"],
      "images/*": ["images/*"],
      "apps/*": ["apps/*"],
      "style/*": ["style/*"]
    },
    "target": "ES6",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["build", "node_modules"]
}
